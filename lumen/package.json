{
  "name": "lumen",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "postinstall": "mkdir -p public/pdf-worker && cp -f node_modules/pdfjs-dist/build/pdf.worker.min.mjs public/pdf-worker/ && cp -f node_modules/react-pdf/node_modules/pdfjs-dist/build/pdf.worker.min.mjs public/pdf-worker/pdf.worker.react-pdf.min.mjs"
  },
  "dependencies": {
    "groq-sdk": "^0.26.0",
    "lucide-react": "^0.395.0",
    "mammoth": "^1.9.1",
    "pdfjs-dist": "^5.3.93",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-markdown": "^9.0.1",
    "react-pdf": "^9.0.0",
    "remark-gfm": "^4.0.1",
    "tailwind-scrollbar": "^3.1.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.29.0",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@types/react-pdf": "^6.2.0",
    "@vitejs/plugin-react": "^4.5.2",
    "autoprefixer": "^10.4.21",
    "eslint": "^9.29.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^16.2.0",
    "postcss": "^8.5.6",
    "tailwindcss": "^3.4.13",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.34.1",
    "vite": "^6.0.0"
  }
